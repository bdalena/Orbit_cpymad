dsR_BPMAll_col     :=  0.0000 ;
dphiR_BPMAll_col   :=  0.0000 ;
dthetaR_BPMAll_col :=  0.0000 ;
dpsiR_BPMAll_col   :=  0.1000 ;
mrexR_BPMAll_col   :=  0.2000 ;
mreyR_BPMAll_col   :=  0.2000 ;

return;
